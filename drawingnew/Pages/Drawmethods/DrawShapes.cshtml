           <div class="col-md-6">
               <label>Name</label>
               <input type="text" class="form-control" id="txt_shapeName" />

           </div>
            <div class="col-md-6">
                <label>Email</label>
                <input type="email" class="form-control" id="txt_shapeemail" />

            </div>
