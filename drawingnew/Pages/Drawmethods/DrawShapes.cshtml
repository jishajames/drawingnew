        <div class="row">
            <div class="col-md-12">
                <canvas id="canvas-circle" class="form-control toggle-circle" style="border:1px solid #000000;"></canvas>

                <div id="button-circle" class="toggle-circle">
                    <input type="button" id="clear" value="Clear">
                </div>
            </div>
        </div>
   </form>
</div>
